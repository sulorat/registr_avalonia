<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="600"
        x:Class="reg.MainWindow"
        Title="reg">
	
	<Grid ColumnDefinitions="*,*,*" RowDefinitions="*,2*,*" ShowGridLines="True">

		<StackPanel Grid.Column="1" Grid.Row="1">
		
		<TextBlock  HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,40,0,0">Name</TextBlock>
		<TextBox Name="name_sign_up"  HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,10,0,0" Height="30" Width="200"></TextBox>
		<TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Margin="0,30,0,0">Password</TextBlock>
		<TextBox Name="password_sign_up" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,10,0,0" Height="30" Width="200"></TextBox>

		</StackPanel>
		<StackPanel Grid.Column="1" Grid.Row="2">
			<Button Name="sign_up" Click="Button_Click" VerticalAlignment="Center" HorizontalAlignment="Center">Sign up</Button>
		</StackPanel>
	</Grid>
	
	
</Window>